{
    "Width": 300,
    "Height": 350,
    "Description": "Creates new columns (max 100) by splitting a specified text column by a given delimiter. Regex is supported.",
    "AutoExecuteOnChange": false,
    "ComputationDescription": {
        "DataframeInputModels": [
            {
                "Name": "input",
                "DisplayName": "input"
            }
        ],
        "DataframeOutputModels": [
            {
                "Name": "output",
                "DisplayName": "output"
            }
        ],
        "AttributeConfigGroupInputDescription": {},
        "ValueInputDescription": {}
    },
    "InputUI": [],
    "InnerInputs": [
        {
            "Name": "column",
            "SubLabel": "text column containing delimiters to split by",
            "__discriminator": "AttributeSelectorInnerInputDescription",
            "FromDataframe": "input",
            "MaxNr": 1,
            "MinNr": 1,
            "AllowNumeric": false,
            "AllowStrings": true,
            "AllowTimestamps": false,
            "AllowBooleans": false,
            "AllowNull": false
        },
        {
            "Name": "delimiter",
            "SubLabel": "e.g. '-', '[0-9]+'",
            "__discriminator": "StringValueInnerInputDescription",
            "DefaultValue": ""
        },
        {
            "Name": "use regex",
            "SubLabel": "whether to parse the delimiter as a regular expression",
            "__discriminator": "CheckboxInnerInputDescription",
            "DefaultValue": false
        }
    ]
}